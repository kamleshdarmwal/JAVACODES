class A
{
	int z;
	public void f1()
	{
       int k=3;
        System.out.println(k);
	}
} 



class B extends A
{
	int z;
	public void f1()
	{
		super.f1();
		//System.out.println(k);

	}

	public void f2()
	{
		int z;
		z=2;
		this.z=3;
		super.z=4;
		System.out.println(z);
		System.out.println(this.z);
		System.out.println(super.z);

	}

}


class Super
{
	public static void main(String [] args)
	{
	B obj=new B();
	obj.f1();
	obj.f2();
	}
}
